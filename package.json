{
  "name": "turborepo-shadcn-ui",
  "version": "1.2.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ui:add": "pnpm --filter @instamate/ui ui:add",
    "deploy": "turbo run build --filter=web --include-dependencies --no-deps"
  },
  "devDependencies": {
    "@instamate/eslint-config": "workspace:*",
    "@instamate/typescript-config": "workspace:*",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}